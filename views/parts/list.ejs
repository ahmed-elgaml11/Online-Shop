<div class="col-md-3 py-3"> <!-- This moves it below the navbar -->
    <h2>Categories</h2>
    <div class="list-group w-120">
      <a href="/products" class="list-group-item list-group-item-action " aria-current="true">
        All products
      </a>
      <% categories.forEach(function(cat){ %>
          <a href="/products/<%= cat.slug %>" class="list-group-item list-group-item-action">
              <%= cat.title %>
          </a>
      <% }) %>
    </div>
  </div>  
